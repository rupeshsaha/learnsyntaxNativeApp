{
  "expo": {
    "name": "learnsyntax",
    "slug": "learnsyntax",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/LearnSyntax.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/LearnSyntax.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
     "android": {
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "ios": {
      "supportsTablet": true
    },
    
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-video"
    ]
  }
}
